constraints: ghc +internal-interpreter,
             ghc-bin +internal-interpreter,
             ghci +internal-interpreter,
             haddock +in-ghc-tree,
             -- any.Cabal installed,
             any.array installed,
             any.base installed,
             any.binary installed,
             any.bytestring installed,
             any.containers installed,
             any.deepseq installed,
             any.directory installed,
             any.exceptions installed,
             any.filepath installed,
             any.ghc-bignum installed,
             any.ghc-boot-th installed,
             any.ghc-compact installed,
             any.ghc-heap installed,
             any.ghc-prim installed,
             any.haskeline installed,
             any.directory installed,
             any.integer-gmp installed,
             any.mtl installed,
             any.parsec installed,
             any.pretty installed,
             any.process installed,
             any.stm installed,
             any.template-haskell installed,
             any.terminfo installed,
             any.text installed,
             any.time installed,
             any.transformers installed,
             any.unix installed,
             any.Win32 installed,
             any.xhtml installed 

allow-newer:
  ghc-paths:Cabal,
  *:base,
  *:ghc-prim,
  tree-diff:time

